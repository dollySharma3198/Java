import java.util.Scanner;
class Max_q8{
    public static void main(String[] args) {
        int arr[]={1,45,56,52,40,80};
        int max=0;
        for(int i:arr)
        {
            if(i>max)
            {
                max=i;
            }
        }
        
        System.out.println("the value of the maximum element in this array is "+max);
    }
}
