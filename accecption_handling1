/// wap to print all prime numner between 1 to 10
//accepction handling 
  //logical error
public class accecption_handling1
{
  public static void main (String[]args)
  {
   //SYNTEX ERRORS
   //a=4 no semicolon 
   //b=10;
    System.out.println (2);
    for (int i = 1; i < 5; i++)
      System.out.println (2 * i + 1);
      
      //run time error /Exception error
      int k;
      Scanner sc=new Scanner(System.in);
      k=sc.nextInt();
      System.out.println("integer part of 1000 divided by k is" +1000/k); //error 
  }
}
